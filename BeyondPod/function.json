{
  "bindings": [
    {
      "authLevel": "function",
      "name": "req",
      "type": "httpTrigger",
      "direction": "in",
      "methods": [
        "post"
      ]
    },
    {
      "name": "$return",
      "type": "http",
      "direction": "out"
    },
    {
      "type": "blob",
      "name": "outputBlob",
      "path": "beyond-pod-data/{rand-guid}.json",
      "connection": "AzureWebJobsStorage",
      "direction": "out"
    }
  ],
  "disabled": true
}